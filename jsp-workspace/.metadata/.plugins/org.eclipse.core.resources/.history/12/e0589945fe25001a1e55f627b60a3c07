<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		// 배열의 크기가 없어도 된다.
		var arr1 = [];
		var arr2 = [ 273, 32, "문자" ];

		console.log(arr1);
		console.log(arr2);
		console.log(arr2[0]);
		console.log(typeof (arr2));
		console.log(typeof (arr2[0]));
		console.log(typeof (arr2[2]));
		console.log(typeof (arr2[5]));

		document.write(arr1 + '<br>');
		document.write(arr2 + '<br>');
		document.write(arr2[0] + '<br>');
		document.write(typeof (arr2) + '<br>');
		document.write(typeof (arr2[0]) + '<br>');
		document.write(typeof (arr2[2]) + '<br>');
		
		// 꼭 모든 방에 데이터가 들어갈 필요는 없다.
		arr1[1] = '30';
		arr1[3] = 3;
		console.log(arr1);
		console.log(arr1[0]);
		console.log(typeof (arr2[1]));
		console.log(typeof (arr2[3]));
		console.log(arr1.length)

		// 향상된 for문. 대신 undefined는 나오지 않는다.
		for (var i in arr1) {
			console.log(arr1[i]);
		}
		
		// Array객체
		var arr3 = Array(10, 20, 30, 40);
		console.log(arr3);
	</script>
</body>
</html>
