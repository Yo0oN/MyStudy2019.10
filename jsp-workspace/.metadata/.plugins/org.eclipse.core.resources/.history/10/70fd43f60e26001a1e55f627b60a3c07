		var product = {
			name : '7D 건조망고',
			type : '당절임',
			components : '망고, 설탕, 기타',
			region : '필리핀'
		}

		console.log(typeof (product));
		console.log(product);
		
		console.log(product.name);
		console.log(product.type);
		
		console.log(product['name']);
		console.log(product['type']);
