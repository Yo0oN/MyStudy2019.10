	document.write('<table width="770" border="1">');
	for (var i = 1; i <= 9; i++) {
		document.write('<tr>');
		document.write('<td>' + i + '단' + '</td>');
		for (var j = 1; j <= 9; j++) {
			document.write('<td>' + i + ' x ' + j + ' = ' + (i * j) + '</td>');
		}
		document.write('</tr>');
	}
	document.write('</table>');
	
