<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		// json
		var student = {};
		console.log(student);

		// 객체 생성시 바로 키와 속성을 넣는것이 아닌,
		// 이후에 넣어도 된다.
		student.name = '홍길동';
		student.hobby = '악기';
		student.spec = '프로그래밍';
		student.hope = '과학자';
		
		console.log(student);
		
		student.test = function() {
			console.log('test 호출');
		};
		
		student.test();
		
		// java의 toString메소드를 만들어보자.
		student.toString = function() {
			var output = '';
			for(var key in this) {
				if (key != 'toString') {
					output += '*' + key + ' : ' + this[key] + '\n';
				}
			}
			return output;
		}
		console.log(student.toString());
		
		student.toString = function(k) {
			var output = '';
			for(var key in this) {
				if (key == k) {
					output += '*' + key + ' : ' + this[key] + '\n';
				}
			}
			return output;
		}
		console.log(student.toString('hope'));
		
		delete(student.spec);
		delete(student.hobby);
		console.log(student.toString('spec'));
	</script>
</body>
</html>
