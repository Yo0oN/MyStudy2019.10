	<script type="text/javascript">
		// 선언과 초기화가 동시에 된 형태.
		var product = {
			// 멤버변수
			name : '7D 건조망고',
			type : '당절임',
			components : '망고, 설탕, 기타',
			region : '필리핀',
			'test1' : '안녕',
			// 멤버 메소드
			eat : function() {
				console.log(name + '맛있게 먹는다.');
			},
			test : function() {
				console.log('맛있다!!');
			}
		}
		
		product.eat();
		product.test();
		
		console.log(product.name);
		console.log(product['name']);
		console.log(product['test1']);
	</script>
