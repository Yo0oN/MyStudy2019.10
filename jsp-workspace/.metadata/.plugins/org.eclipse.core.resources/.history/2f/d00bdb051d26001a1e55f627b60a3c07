<script type="text/javascript">
	// 생성자 함수
	function Student (name, kor, mat) {
		this.name = name;
		this.kor = kor;
		this.mat = mat;
		
		this.getSum = function() {
			return this.kor + this.mat;
		};
	}
	
	var stu = new Student('홍길동', 85, 96);
</script>
