<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	// func1은 함수가아닌 변수다!
	// 단지 함수를 넣어서 사용하기 때문에 자료형이 function일뿐!
	var func1 = function() {
		console.log('Hello Function');
	}
	
	var func2 = function(data1) {
		console.log('data1 : ' + data1);
	}
</script>
</head>
<body>
	<script type="text/javascript">
		func1();
		console.log(typeof(func1));
		console.log(func1);
		
		var func2() = func1;
		func2();
		console.log(typeof(func2));
		console.log(func2);
		
		func3();
	</script>
</body>
</html>
