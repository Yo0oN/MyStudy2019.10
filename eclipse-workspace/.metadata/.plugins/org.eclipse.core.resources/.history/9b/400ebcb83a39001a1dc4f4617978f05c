package MyQueue;

public class ArrayEx03 {

	public static void main(String[] args) {
		// 배열 응용하여 기수 변환
		String number = "504861"; // 10진수

		int numberlength = number.length();
		int result = 0;

		for (int i = 0, j = numberlength; i < numberlength; i++, j--) {
			result += number.charAt(i) * (10 ^ j);
		}
		System.out.println(result);
	}

}
