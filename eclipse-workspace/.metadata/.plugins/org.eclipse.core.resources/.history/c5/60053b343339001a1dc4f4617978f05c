package MyQueue;

import java.util.Random;

public class ArrayEx01 {
	public static void main(String[] args) {
		int[] array1 = new int[10];

		for (int i = 0; i < 10; i++) {
			array1[i] = new Random().nextInt(10);
		}
		for (int i : array1) {
			System.out.println(i);
		}
		// 배열에서 최댓값 구하기
		int max = array1[0];
		for (int i = 1; i < 10; i++) {
			if (array1[i] > max) {
				max = array1[i];
			}
		}
	}

}
